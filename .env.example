# .env.example - Copy this to .env and fill in your Schwab API credentials
# Get these from https://developer.schwab.com/
SCHWAB_APP_KEY=your_app_key_here
SCHWAB_APP_SECRET=your_app_secret_here
SCHWAB_CALLBACK_URL=https://127.0.0.1:8443/callback
SCHWAB_TOKEN_PATH=./schwab_tokens.json

# Flask Configuration
FLASK_SECRET_KEY=your_secret_key_here
FLASK_DEBUG=True